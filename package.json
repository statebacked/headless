{
  "name": "@statebacked/headless",
  "version": "0.1.1",
  "description": "Headless backend components",
  "scripts": {
    "prettier": "prettier --write src",
    "build": "tsc --outDir build",
    "prepublishOnly": "npm run build && rollup --config rollup.config.mjs && tsc --emitDeclarationOnly --declaration --outDir types && echo '{ \"type\": \"commonjs\" }' > lib/package.json",
    "install-feature": "npm run build && node dist/scripts/install.js",
    "smply": "smply",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "type": "module",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "types": "types/index.d.ts",
  "bin": "./build/scripts/install.js",
  "exports": {
    ".": {
      "types": {
        "import": "./types/index.d.ts",
        "require": "./types/index.d.ts"
      },
      "default": {
        "import": "./esm/index.js",
        "require": "./lib/index.js"
      }
    }
  },
  "files": [
    "build",
    "lib",
    "esm",
    "types"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/statebacked/headless.git"
  },
  "keywords": [
    "State Backed",
    "state-backed",
    "headless",
    "state machine"
  ],
  "author": "Adam Berger <adam@statebacked.dev>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/statebacked/headless/issues"
  },
  "homepage": "https://github.com/statebacked/headless#readme",
  "peerDependencies": {
    "react": ">=16.0.0"
  },
  "dependencies": {
    "@statebacked/client": "^0.1.37",
    "@statebacked/machine": "^0.6.0",
    "smply": "^0.1.17",
    "xstate": "^4.38.2"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "^11.1.5",
    "@types/node": "^20.8.5",
    "@types/react": "^18.2.28",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "rollup": "^3.28.1",
    "typescript": "^5.2.2"
  }
}
